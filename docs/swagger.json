    "/api/monitor/{:action}": {
      "get": {
        "tags": [
          "Mirror Control",
          "API"
        ],
        "summary": "Control the connected display, turning it on or off.",
        "parameters": [
          {
            "name": ":action",
            "in": "path",
            "required": true,
            "allowEmptyValue": true,
            "schema": {
              "type": "string"
            },
            "examples": {
              "on": {
                "summary": "Turn on",
                "value": "on"
              },
              "off": {
                "summary": "Turn off",
                "value": "off"
              },
              "toggle": {
                "summary": "Toggle",
                "value": "toggle"
              },
              "status": {
                "summary": "Status",
                "value": "status"
              }
            }
          }
        ],
        "security": [
          {
            "apikey": []
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "example": {
                  "success": true,
                  "monitor": "on"
                }
              }
            }
          }
        }
      }
    },
    "/api/monitor/": {
      "post": {
        "tags": [
          "Mirror Control",
