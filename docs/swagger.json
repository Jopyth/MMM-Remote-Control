                "example": {
                  "success": true,
                  "query": {
                    "data": "classes"
                  },
                  "data": {
                    "test": {
                      "toggle": "calendar"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "",
            "content": {
              "application/json": {
                "example": {
                  "success": false,
                  "message": "Invalid value test provided in request. Use /api/classes to see actual values"
                }
              }
            }
          }
        }
      }
    },
    "/api/command/{:value}": {
      "get": {
        "tags": [
          "Mirror Control",
          "API"
        ],
        "summary": "Execute command saved in config",
